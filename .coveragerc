[run]
dynamic_context = test_function
branch = True
omit =
    venv/*
    .tox/*
    tests/*
    setup.py
    tb-ops/cmd/migrations/*

[report]
show_missing = True
exclude_lines =
    pragma: no cover

    def __repr__
    if self\.debug

    raise AssertionError
    raise NotImplementedError

    async def __ensure_pool(self) -> asyncpg.Pool:

    if 0:
    if __name__ == .__main__.:

[html]
show_contexts = True
